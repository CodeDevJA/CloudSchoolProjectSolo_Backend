{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "http": {
            "routePrefix": "api"
        }
    },
    "cors": {
        "supportCredentials": true,
        "allowedOrigins": [
            "http://localhost:3000",
            "http://localhost:5173",
            "https://codedevja.github.io"
        ]
    }
}